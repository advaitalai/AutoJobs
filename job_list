from bs4 import BeautifulSoup
from utils import html

class JobList:
    
    def __init__(self, url) -> None:
        self.soup = BeautifulSoup(html.get_source(url), 'html.parser')
    
    def get_soup(self):
        return self.soup
        
if __name__ == '__main__':
    url = 'https://miro.com/careers/open-positions/?selectedTeam=Product#jobList'
    job_list = JobList(url).get_soup()
    job_list