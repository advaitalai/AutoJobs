from bs4 import BeautifulSoup
import utils

class JobList:
    
    def __init__(self, url) -> None:
        self.soup = BeautifulSoup(utils.get_html_src(url), 'html.parser')
    
    def get_title(self):
        print(self.soup.title)
        
